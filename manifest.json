{
  "name": "Mob2Pin",
  "version": "1.0",
  "description": "Create pinboard bookmarks for all Chrome Mobile bookmarks",
  "permissions": [
    "bookmarks",
    "tabs"
  ],
  "browser_action": {
      "default_title": "Mob2Pin",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
